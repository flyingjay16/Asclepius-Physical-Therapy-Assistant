<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    app:cardCornerRadius="12dp"
    android:elevation="90dp"
    android:layout_margin="7dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="200dp"
        android:orientation="vertical"
        android:background="#3F3C3C">

        <TextView
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cardTime"
            android:text="TIME"
            android:textSize="30dp"
            android:textColor="#ffff">
        </TextView>
        <TextView
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cardPercentFacePulls"
            android:text="PERCENT FACE PULLS"
            android:textSize="20dp"
            android:textColor="#ffff">
        </TextView>
        <TextView
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cardPercentLowerTraps"
            android:text="PERCENT LOWER TRAPS"
            android:textSize="20dp"
            android:textColor="#ffff">
        </TextView>
        <TextView
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cardPercentRotatorCuffs"
            android:text="PERCENT ROTATOR CUFFS"
            android:textSize="20dp"
            android:textColor="#ffff">
        </TextView>
        <TextView
            android:layout_margin="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/cardPercentSwimmers"
            android:text="PERCENT SWIMMERS"
            android:textSize="20dp"
            android:textColor="#ffff">
        </TextView>

    </LinearLayout>

</androidx.cardview.widget.CardView>